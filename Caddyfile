ratregistry.daddigital.com {
	reverse_proxy localhost:5000
	encode gzip
}

ratuireg.daddigital.com {
	# Registry API calls - proxy to registry
	handle /v2/* {
		reverse_proxy localhost:5000
	}
	
	# Everything else - proxy to UI
	handle {
		reverse_proxy localhost:5001
	}
	encode gzip
}

rat.gumshoe.au {
	reverse_proxy localhost:8083
	encode gzip
}


rattaps.daddigital.com {
	reverse_proxy localhost:8084
	encode gzip
}
