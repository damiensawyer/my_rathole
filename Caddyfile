registry.datquehuong.com {
	# Docker Registry v2 API - disable compression for blob uploads
	handle /v2/*/blobs/uploads/* {
		reverse_proxy localhost:8080
	}

	# Other registry API and UI - can use compression
	handle {
		reverse_proxy localhost:8080
		encode gzip
	}
}

devtaps.datquehuong.com {
	reverse_proxy localhost:8082
}

prodtaps.datquehuong.com {
	reverse_proxy localhost:8083
}
